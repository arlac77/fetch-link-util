{
  "name": "fetch-link-util",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/util.mjs"
  },
  "description": "helper to extract fetch link header values (paging/RFC-8288)",
  "keywords": [
    "RFC-8288",
    "fetch",
    "link",
    "paging",
    "rel"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.8.0",
    "documentation": "^13.2.5",
    "semantic-release": "^17.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/fetch-link-util.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/fetch-link-util/issues"
  },
  "homepage": "https://github.com/arlac77/fetch-link-util#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
